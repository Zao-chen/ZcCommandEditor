    background-color: #f8b156;
}

QMenuBar::item:pressed {
    background-color: #c72e1d;
}


QLineEdit {
    border: 2px solid #f7931e;
    border-radius: 3px;
    background-color: #FFFCF3;
    padding: 2px;
    font-size: 16px;
}


QTextEdit {
    border: 2px solid #f7931e;
    border-radius: 3px;;
    padding: 5px;
    font-size: 16px;
    background-color: #FFFCF3;
}

QTableView {
    border: 2px solid #f7931e;
    border-radius:  3px;
    background-color: #FFFCF3;
}

QTableView::item {
    padding: 5px;
    font-size: 16px;
}



QPushButton {
    background-color: #FFBC33;
    color: #ffffff;
    border: none;
    border-radius: 20px;
    padding: 8px 20px;
    font-size: 16px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #f8b156;
}

QPushButton:pressed {
    background-color: #c72e1d;
}


QComboBox {
    border: none;
    border-radius: 20px;
    background-color: #FFBC33;
    color: #ffffff;
    font-size: 16px;
    font-weight: bold;
    padding: 8px 20px;
    padding-right: 4px; /* 右侧留出一定空间 */
    selection-background-color: #f8b156; /* 选中项背景色 */
    selection-color: #ffffff; /* 选中项前景色 */
    outline: none;
}

QComboBox:hover {
    background-color: #f8b156;
}

QComboBox::drop-down {
    width: 0; /* 去掉下拉箭头 */
}

QCheckBox {
    color: #FFBC33;
    font-size: 16px;
    font-weight: bold;
    padding-left: 20px;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
}

QCheckBox::indicator::unchecked {
    border: 2px solid #FFBC33;
    background-color: #FFFEF9;
}

QCheckBox::indicator::unchecked:hover {
    border: 2px solid #f8b156;
}

QCheckBox::indicator::checked {
    border: 2px solid #FFBC33;
    background-color: #FFBC33;
}

QCheckBox::indicator::checked:hover {
    background-color: #f8b156;
}
